<div class="row">
  <div class="col-md-3">
    <%= render 'layouts/sidebar' %>
  </div>

  <div class="col-md-9">
    <div class="jumbotron">
      <h3>
        Military Harbor
      </h3>
      <p>Items with '' label will be free shipping service! Check them now.</p>
    </div>

    <div class="row">
      <% @products.each do |product| %>
        <div class="col-sm-4 col-lg-4 col-md-4">
          <%= render 'products/product', product: product %>
        </div>
      <% end %>
    </div>
  </div>
</div>
